<%= form_for(@workout_session) do |f| %>
  <ul>
  <% @workout_session.errors.full_messages.each do |error| %>
    <li><%= error %></li>
  <% end %>
  </ul>
  <p>
    <%= f.label :workout_focus %><br />
    <%= f.text_area :workout_focus %>
  </p>
  <p>
    <%= f.label :exercises %><br />
    <%= f.text_area :exercises %>
  </p>
  <p>
    <%= f.label :sets %><br />
    <%= f.text_area :sets %>
  </p>
  <p>
    <%= f.label :reps %><br />
    <%= f.text_area :reps %>
  </p>
  <p>
    <%= f.label :weight %><br />
    <%= f.text_area :weight %>
  </p>
  <p>
    <%= f.label :exercise_time %><br />
    <%= f.text_area :exercise_time %>
  </p>
  <p>
    <%= f.label :workout_time %><br />
    <%= f.text_area :workout_time %>
  </p>
  <p>
    <%= f.label :workout_intensity %><br />
    <%= f.text_area :workout_intensity %>
  </p>
  <p>
    <%= f.label :workout_notes %><br />
    <%= f.text_area :workout_notes %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
